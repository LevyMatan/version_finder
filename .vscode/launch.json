{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Electron Main Process",
        "type": "node",
        "request": "attach",
        "port": 9229,
        "timeout": 30000,
        "stopOnEntry": false,
        "sourceMaps": true,
        "protocol": "inspector",
        "restart": true,
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "program": "${workspaceFolder}/main.js"
      }
    ]
}