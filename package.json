{
  "name": "version_finder",
  "version": "1.0.0",
  "description": "App to assist developer find the first version including a specific commit in a Git Repository with multiply submodules.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "docs": "jsdoc -r .",
    "package": "electron-packager . VersionFinder --platform=darwin --arch=x64 --overwrite --icon=icon.icns --out=release-builds --prune=false",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matan Levy",
  "license": "MIT",
  "devDependencies": {
    "electron": "^30.0.9",
    "electron-packager": "^17.1.2",
    "jsdoc": "^4.0.3",
    "nodemon": "^3.1.3",
    "simple-git": "^3.24.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  }
}
